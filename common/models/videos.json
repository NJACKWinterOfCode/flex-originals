{
  "name": "videos",
  "plural": "videos",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "hidden": [
    "videoOwnerId"
  ],
  "properties": {
    "title": {
      "type": "string",
      "required": true,
      "default": "Untitled"
    },
    "videoMeta": {
      "type": "object",
      "required": true,
      "default": {}
    },
    "videoFile": {
      "type": "string",
      "required": true
    },
    "visibility": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "description": {
      "type": "string"
    },
    "thumbImage": {
      "type": "string"
    },
    "gif": {
      "type": "string"
    },
    "thumbnails": {
      "type": "object"
    },
    "tags": {
      "type": "string"
    },
    "playlist": {
      "type": "object"
    },
    "location": {
      "type": "string"
    },
    "category": {
      "type": "string"
    },
    "allowComments": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "agerestriction": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "ratings": {
      "type": "number"
    },
    "licence": {
      "type": "string",
      "required": true,
      "default": "Flex Originals"
    },
    "likedPrivate": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "published": {
      "type": "date",
      "required": true,
      "default": "$now"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "videoOwnerId"
    },
    "videoAnalytics": {
      "type": "hasOne",
      "model": "videoAnalytic",
      "foreignKey": "videoId"
    },
    "videoLikes": {
      "type": "hasMany",
      "model": "videoLike",
      "foreignKey": "videoLike",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "publish"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "deleteItem"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "getVideoInfo"
    }
  ],
  "methods": {}
}
